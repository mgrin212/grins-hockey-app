{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [ {
      "source": "**",
      "function": "sendWebApp"
    } ],
    "predeploy": ["npm run build"]
  },
  "functions": {
    "source": "functions"
  }
}
